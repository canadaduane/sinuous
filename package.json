{
  "name": "sinuous",
  "version": "0.30.0",
  "description": "ðŸ§¬ Small, fast, reactive render engine",
  "module": "src/index.js",
  "main": "src/index.js",
  "types": "src/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "import": "./src/index.js"
    },
    "./h": {
      "import": "./src/h.js"
    },
    "./babel-plugin-htm": {
      "import": "./src/babel-plugin-htm.js",
      "require": "./dist/babel-plugin-htm.js"
    },
    "./htm": {
      "import": "./src/htm.js"
    },
    "./hydrate": {
      "import": "./src/hydrate.js"
    },
    "./map": {
      "import": "./src/map.js"
    },
    "./observable": {
      "import": "./src/observable.js"
    },
    "./template": {
      "import": "./src/template.js"
    }
  },
  "files": [
    "src",
    "!**/test/**"
  ],
  "scripts": {
    "lint": "eslint 'src/*.js'",
    "test": "cross-env COVERAGE=true npm run test:htm && karma start",
    "test:htm": "tape -r esm test/htm/*.js | faucet",
    "build": "rollup -i src/babel-plugin-htm.js -f cjs -d dist",
    "prepublishOnly": "npm run lint"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "browser": true,
      "es6": true,
      "node": true
    },
    "extends": [
      "eslint:recommended",
      "plugin:import/warnings"
    ],
    "parserOptions": {
      "ecmaVersion": 9,
      "sourceType": "module"
    },
    "rules": {
      "semi": "error",
      "no-unused-vars": [
        "error",
        {
          "varsIgnorePattern": "^hs?|ds?$"
        }
      ]
    }
  },
  "repository": "luwes/sinuous",
  "keywords": [
    "functional",
    "reactive",
    "declarative"
  ],
  "author": "Wesley Luyten <me@wesleyluyten.com> (https://wesleyluyten.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/luwes/sinuous/issues"
  },
  "homepage": "https://github.com/luwes/sinuous#readme",
  "devDependencies": {
    "@rollup/plugin-alias": "^4.0.3",
    "@rollup/plugin-commonjs": "^24.0.1",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "ansi-colors": "^4.1.3",
    "cross-env": "^7.0.3",
    "eslint": "^8.33.0",
    "eslint-plugin-import": "^2.27.5",
    "esm": "^3.2.25",
    "faucet": "^0.0.3",
    "ispy": "^0.1.2",
    "karma": "6.4.1",
    "karma-coverage": "2.2.0",
    "karma-firefox-launcher": "^2.1.2",
    "karma-rollup-preprocessor": "7.0.7",
    "karma-tap": "^4.2.0",
    "karma-tap-pretty-reporter": "^4.2.0",
    "minimist": "^1.2.7",
    "rollup": "^3.13.0",
    "rollup-plugin-istanbul": "^4.0.0",
    "tape": "5.6.3",
    "tape-browser": "^4.13.0",
    "typescript": "^4.9.5"
  }
}
